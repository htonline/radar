<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#fff"
    android:layout_width="@dimen/mydialogWidth"
    android:layout_height="@dimen/mydialogHeight"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ib_login"/>
            <TextView
                android:id="@+id/my_dialog_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="扫描二维码上传" >
            </TextView>





            <ImageButton
                android:src="@drawable/scan_icon16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/iv_scanicon"
                />

        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:text="线路名"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_dqbh"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="挡墙类型"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_dqtype"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:text="挡墙高度"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_dqheight"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="天气"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_dqwidth"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="片石尺寸"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_dqthk"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="行别"
                    android:layout_width="80dp"
                    android:layout_margin="5dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_cxbh"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:text="挡墙倾斜度"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_cxorient"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="挡墙起止里程"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_cxstart"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="表面破损情况"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_cxstop"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="表面植被情况"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:id="@+id/tv_dqloc"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#000"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:text="采集人员"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <EditText
                    android:id="@+id/et_detect_username"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:imeOptions="actionDone"
                    android:hint="请输入姓名"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <TextView
                    android:text="主机序号"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"

                    />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <EditText
                    android:id="@+id/et_mainServerNum"
                    android:layout_width="80dp"
                    android:layout_margin="5dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:imeOptions="actionDone"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    android:hint="请输入主机序号"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:text="雷达数据"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_margin="5dp"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:textSize="@dimen/textNormSize"
                        android:textColor="#000"
                        />
                    <ImageButton
                        android:src="@drawable/serchfile16"
                        android:id="@+id/my_dialog_addFile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000"
                        android:baselineAligned="false"
                        android:layout_gravity="center" />
                </LinearLayout>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:id="@+id/tv_pathoffile"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_margin="5dp"
                        android:layout_gravity="center"
                        android:textSize="@dimen/textNormSize"
                        android:textColor="#000"
                        />
                    <ImageButton
                        android:id="@+id/ib_uploadFile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000"
                        android:baselineAligned="false"
                        android:layout_gravity="center" />
                </LinearLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                <TextView
                    android:text="上传现场图片"
                    android:layout_gravity="center"
                    android:layout_width="80dp"
                    android:layout_margin="5dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/textNormSize"
                    android:textColor="#000"
                    /></LinearLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    />
                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    >
                    <ImageButton
                        android:id="@+id/ib_upload_pic_01"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                    </ImageButton>
                    <ImageButton
                        android:id="@+id/ib_upload_pic_02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                    </ImageButton>
                    <ImageButton
                        android:id="@+id/ib_upload_pic_03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                    </ImageButton>
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/et_remark"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:layout_margin="5dp"
                    android:imeOptions="actionDone"
                    android:textSize="@dimen/textNormSize"
                    android:hint="请输入备注" />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000"
                />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/btn_my_dialog_cancel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="#000"
            android:baselineAligned="false"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:text="返回" />

        <Button
            android:id="@+id/btn_my_dialog_upload"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="#000"
            android:baselineAligned="false"
            android:layout_weight="1"
            android:layout_marginRight="5dp"
            android:text="确定上传" />

    </LinearLayout>





</LinearLayout>